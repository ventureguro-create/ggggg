{
  "name": "fomo-backend",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "dev:node": "tsx watch src/server.ts",
    "dev:python": "cd /app/backend && python server.py",
    "dev:parser": "cd /app/twitter-parser-v2 && yarn dev",
    "dev:all": "concurrently -k -n PYTHON,NODE,PARSER -c blue,green,yellow \"yarn dev:python\" \"yarn dev:node\" \"yarn dev:parser\"",
    "start": "node dist/server.js",
    "build": "tsc -p tsconfig.json",
    "lint": "eslint src --ext .ts",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "dependencies": {
    "@fastify/cors": "^11.2.0",
    "@fastify/websocket": "^11.2.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node-cron": "^3.0.11",
    "axios": "^1.13.5",
    "dotenv": "^17.3.1",
    "ethers": "^6.0.0",
    "fastify": "^5.7.4",
    "ioredis": "^5.9.2",
    "jsonwebtoken": "^9.0.3",
    "mongodb": "^7.1.0",
    "mongoose": "^9.1.6",
    "node-cron": "^4.2.1",
    "openai": "^6.18.0",
    "telegram": "^2.26.22",
    "uuid": "^13.0.0",
    "ws": "^8.19.0",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@types/node": "^25.2.2",
    "@types/uuid": "^11.0.0",
    "@types/ws": "^8.18.1",
    "@typescript-eslint/eslint-plugin": "^8.53.0",
    "@typescript-eslint/parser": "^8.53.0",
    "@vitest/ui": "^4.0.18",
    "concurrently": "^9.2.1",
    "eslint": "^9.39.2",
    "pino-pretty": "^13.1.3",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "vitest": "^4.0.18"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
